    $(document).ready(function(){
      function addmainlinks(){
            $(this).addClass("hovering");
            }
        function removemainlinks(){
            $(this).removeClass("hovering");
            }
        var mainlinksConfig = {
             interval: 200,
             sensitivity: 4,
             over: addmainlinks,
             timeout: 300,
             out: removemainlinks
        };
