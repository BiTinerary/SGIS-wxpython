<script type='text/javascript'>
    function jWPlayerConfiguration()
    {
        var duration = 250;
        var playerConfiguration =
        {
            flashStream: 'http://shophqvodhd-vh.akamaihd.net/z/hd/onair_05_18_14_16_00_01/onair_05_18_14_16_00_01_,150K,250K,500K,850K,1200K,.mp4.csmil/manifest.f4m',
            autoStart: false,
            hlsStream: 'http://shophqvodhd-vh.akamaihd.net/i/hd/onair_05_18_14_16_00_01/onair_05_18_14_16_00_01_,150K,250K,500K,850K,1200K,.mp4.csmil/master.m3u8',
            playerName: 'shophqplayer',
            start: 2134,
            duration: duration,
            ccText: '',
            height: 354,
            width: 472,
            onPlay: videoPlay,
            onComplete: videoComplete,
            sPlayPageName: 'Product 601-951--**--' + duration + '--**--Product Detail Video Player--**--0--**--' + Math.floor(new Date().getTime() / 1000) + '--**--S0L0',
            sPlayPageTitle: 'Product 601-951'
        };
        shopHq.video.watchOnDemandVideo(playerConfiguration);
        initializeVarablesForProductVideoSiteCatTracking(playerConfiguration);  //BaseOfferPage.js
    }

    var videoExists = 'True';

    if (videoExists == 'True') 
    {
        Event.observe(window, 'load', jWPlayerConfiguration);
        document.getElementById('btnVideo').src = '/images/buttons/Video_Active.jpg';
        document.getElementById('btnVideo').removeAttribute('disabled');
        if (window.location.href.toLowerCase().indexOf("#videofrompg") > 0) 
        {
            Event.observe(window, 'load', btnVideo_Click());
        }
    }
            
    HideButtonContainer(videoExists);
    pppanel1 = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_ppPanel');
    if (this.skus != null && skus.length == 1) {
        sku = skus[0];
        if (sku._WarantyCode !=null && sku._WarantyCode !="") {
            pppanel1.style.display = "block";
            document.getElementById("ppvale").innerHTML = sku._WarantyPrice;
        }
    }

    sizeDropDown = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_ProductSizes');
    colorDropDown = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_ProductColors');
    pricepanelInd = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_pricepanelInd');
    retailpanel = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_pricepanel');
    ddlqty = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_ddlqty');

    valuePayPanel = $('BaseMasterMiddlePH_ExOfferDetails_ValuePayPanel');
    autoDeliveryDropDown = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_ProductAutoDelivery');
    autoDeliverySkuPanel = $('BaseMasterMiddlePH_ExOfferDetails_AutoDeliverySkuPanel');

    
    var bvUserId = null;
    

    $BV.configure('global',
    {
        productId: "601-951",
        userToken: bvUserId,
        doShowContent: function ()
        {
            openReviewsTab();
        }
    });
    $BV.ui("rr", "show_reviews");

    PRTabIndex = '{tab_index}';




    //
    CompletePriceInd = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_CompletePriceInd'); //
    ShortPricePanelInd = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_ShortPricePanelInd'); //
    ValuePayPanelInd = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_ValuePayPanelInd'); //
    RetailValuePanelInd = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_RetailValuePanelInd');
    RegularPricePanelInd = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_RegularPricePanelInd'); //
    ValueShippingInd = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_ValueShippingInd');
    LtCurrentPrice = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_lblCurrentPrice');
    LtCurrentPriceLabel = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_lblCurrentPriceLabel');
    lblSHPrice = document.getElementById("BaseMasterMiddlePH_ExOfferDetails_lblSHPrice");
    completePricepanel = document.getElementById('BaseMasterMiddlePH_ExOfferDetails_CompletePricePanel');
    InitializeImageViewer();
    InitializeColorSizeDropDown();
    SizeValueFromDDL();

    var req = null;
    var isSubmitted = false;
        


    function AddToCartAndCheckout()
    {
        if (isAlreadyWorking()) {
            return;
        }
            
        isQuickBuy = false;
        if (SetSelectedSku() && selectedSku) {
            if (ValidateAutoDelivery()) {
                var programString = "";
                if (autoDeliveryDropDown != null && autoDeliveryDropDown.value > -1) {
                    programString = "&programcode=" + autoDeliveryDropDown.value;
                }
                try {
                    var Sku = offerFamilyID + '-' + selectedSku._colorCode + '-' + selectedSku._sizeCode;

                    if (!shopHq.minicart.enableMiniCart) {
                        s.events = 'scAdd,scOpen';
                        s.products = ';' + offerFamilyID + ';;;;' + 'evar13=' + offerID;
                        s.linkTrackVars = 'products,events';
                        s.linkTrackEvents = 'scAdd,scOpen';
                        s.tl(this, 'o', 'Add to Cart');
                    }    
                } catch (e) {

                }
                var url = '/offer/default.aspx?action=AddToCart&offercode=601-951&selectedsku=' + selectedSku._id + '&qty=' + ddlqty.value + '&track=0' + programString + '&rand=' + (new Date()).getMilliseconds();
                    
                if ($('HasProtectionPlan') != 'undefined' && $('HasProtectionPlan').checked) {
                    url = url + '&protectionplan=true'
                }
                    
                // Submit the form using Ajax or full page submit based on the Mini Cart setting in the db
                if (shopHq.minicart.enableMiniCart) {
                    shopHq.minicart.displayMiniCart(url);
                }
                else {
                    var f = document.forms[0];
                    f.action = url;
                    isSubmitted = true;
                    f.submit();
                }
                    
            }
        }
    }

    function isAlreadyWorking() {
        if (isSubmitted) {
            return true;
        }
        return false;
    }
    function addPromoOfferToCart() {

        isQuickBuy = false;
        if (isAlreadyWorking()) {
            return;
        }
        if (SetSelectedSku() && selectedSku) {
            if (ValidateAutoDelivery()) {
                var programString = "";
                if (autoDeliveryDropDown != null && autoDeliveryDropDown.value > -1) {
                    programString = "&programcode=" + autoDeliveryDropDown.value;
                }

                var url = '/offer/default.aspx?action=AddToCart&offercode=601-951&selectedsku=' + selectedSku._id + '&qty=' + ddlqty.value + '&track=0' + programString + '&rand=' + (new Date()).getMilliseconds();
                if ($('HasProtectionPlan') != 'undefined' && $('HasProtectionPlan').checked) {
                    url = url + '&protectionplan=true'
                }
                isQuickBuy = false;
                adjustQuantity = true;
                showCartFlyout = true;

                if (typeof XMLHttpRequest != 'undefined')
                    req = new XMLHttpRequest();
                else if (window.ActiveXObject)
                    req = new ActiveXObject("Microsoft.XMLHTTP"); 	// code for IE6, IE5
                if (req != null) {
                    req.open("GET", url, true);
                    req.onreadystatechange = CartCallBack;
                    isSubmitted = true;
                    req.send(null);
                }
                s.events = 'scAdd,scOpen';
                s.products = ';' + offerFamilyID + ';;;;' + 'evar13=' + offerID;
                s.linkTrackVars = 'offers,events';
                s.linkTrackEvents = 'scAdd,scOpen';
                s.tl(this, 'o', 'Add to Cart');
            }
        }
    }


    function goToCheckout() {
        if (req.readyState == 4) {
            if (req.status == 200) {
                isSubmitted = true;
                var ptc = "/Basket/?target=Basket&action=None";
                window.location = ptc;
            }
        }
    }

    function ValidateAutoDelivery() {
        //assume it is valid.  
        var valid = false;
        var noDeliverySeleced = false;

        //Check to make sure the autoDelviery was selected on this item
        if (autoDeliveryDropDown != null && autoDeliveryDropDown.value == -2) {
            noDeliverySeleced = true;
        }
        else if (autoDeliveryDropDown != null && autoDeliveryDropDown.value > -1) {
            //check to make sure we have an auto delivery on this item
            if (selectedSku._autoDeliveryPrograms != null) {

                for (var i = 0; i < selectedSku._autoDeliveryPrograms.length; i++) {

                    if (selectedSku._autoDeliveryPrograms[i].ProgramId == autoDeliveryDropDown.value) {
                        //we found our program
                        valid = true;
                        break;
                    }
                }
            }
        }
        else {
            valid = true;
        }

        if (!valid) {
            var messageArea = document.getElementById('SkuMessage');
            messageArea.style.display = "";
            if (noDeliverySeleced)
                messageArea.innerHTML = 'Please select a Delivery Program';
            else
                messageArea.innerHTML = 'This item combination is not available with this program';
        }
        else {
            var messageArea = document.getElementById('SkuMessage');
            messageArea.style.display = "none";
            messageArea.innerHTML = '';

        }

        return valid;
    }

    function protectionPlanCheckedChanged()
    {
        var warrantyPrice = 0.0;
        if (this.skus != null && skus.length > 0)
        {
            sku = skus[0];
            if (sku._WarantyPrice > 0)
            {
                warrantyPrice = sku._WarantyPrice;
            }
        }
        if ($('HasProtectionPlan').checked)
        {
            s.events = 'event25';
            s.products = ';' + offerFamilyID + ';;;;' + 'evar13=' + offerID + 'evar37=SVV_Product Detail' + ';event25=' + warrantyPrice;
            s.linkTrackVars = 'products,events';
            s.linkTrackEvents = '';
            s.tl(this, 'o', 'Protection Plan');
        }
    }

    function QuickBuyProduct() {

        if (isAlreadyWorking()) {
            return;
        }

        if (autoDeliveryDropDown != null && autoDeliveryDropDown.value == -2) {

            isSubmitted = false;
            isQuickBuy = false;
            var messageArea = document.getElementById('SkuMessage');
            messageArea.style.display = "";
            messageArea.innerHTML = 'Please select a Delivery Program';

        }
        else {
            if (SetSelectedSku() && selectedSku) {

                var programString = "";
                if (autoDeliveryDropDown != null && autoDeliveryDropDown.value > -1) {
                    programString = "&programcode=" + autoDeliveryDropDown.value;
                }

                //var url = '/product/?action=quickbuy&familyid=J176344&selectedsku=' + selectedSku._id + '&qty=1' + programString + '&rand=' + (new Date()).getMilliseconds();
                var url = '/offer/default.aspx?action=quickbuy&OfferCode=601-951&selectedsku=' + selectedSku._id + '&qty=' + ddlqty.value + programString + '&rand=' + (new Date()).getMilliseconds();

                if ($('HasProtectionPlan') != 'undefined' && $('HasProtectionPlan').checked) {
                    url = url + '&protectionplan=true'
                }
                
                isQuickBuy = true;
                adjustQuantity = false;
                showCartFlyout = false;
                isSubmitted = true;
                if (typeof XMLHttpRequest != 'undefined')
                    req = new XMLHttpRequest();
                else if (window.ActiveXObject)
                    req = new ActiveXObject("Microsoft.XMLHTTP"); 	// code for IE6, IE5
                if (req != null) {
                    req.open("GET", url, true);
                    req.onreadystatechange = CartCallBack;

                    req.send(null);
                }
            }
        }
    }

    function AcceptAddAndSave(isAccepted) {
        var url = '/offer/default.aspx?offercode=&addandsavefamilyid=J176344';
        if (isAccepted && SetSelectedSku()) {
            url += '&selectedsku=' + selectedSku._id + '&qty=1&action=ViewAddAndSaveAcceptBasket&addandsavepromoid=&addandsavetype=NONE';
            isSubmitted = true;
            window.location = url;
        } else if (!isAccepted) {
            url += '&action=ViewAddAndSaveDeclineBasket&addandsavepromoid=&addandsavetype=NONE';
            if ($('HasProtectionPlan') != 'undefined' && $('HasProtectionPlan').checked) {
                url = url + '&protectionplan=true'
            }
            isSubmitted = true;
            window.location = url;
        }
        
    }
    
    if (autoDeliveryDropDown != null && Prototype.Browser.IE && Prototype.Browser.IEVersion < 9) {
        for (var i = 0; i < autoDeliveryDropDown.options.length; i++) {

            var str = autoDeliveryDropDown.options[i].text.lastIndexOf('-');

            if (parseInt(str) != -1) {

                autoDeliveryDropDown.options[i].innerHTML = autoDeliveryDropDown.options[i].text.substr(0, str);
            }

        }
    }

</script>

	    </div>
	     <!-- End Left Column content -->
	</div>
	   
    

