define('pageProperties', function(){
	
	// Construct object
    function PageProperties() {

        // Assign context
        var pageProperties = this;
        $.extend(true, pageProperties, {"addToCartHandler":0,"addToCartLightboxEnabled":false,"checkoutRewriteCart2014Enabled":false,"cartDrawerTest2014Enabled":false,"totalCostsEnabled":false,"financialSnapshotDisplayed":false,"showPricePerMonth":true,"bazaarvoiceEnabled":true,"freshStart":false,"pdpExperienceMessageEnabled":true,"warrantyTestingCapabilitiesEnabled":false,"warrantyInConfirmationEnabled":false,"bazaarVoiceAuthorUrl":"http://reviews.fingerhut.com/profiles/3526/\u003cauthorId\u003e/profile.htm","bazaarVoiceTermsAndConditionsUrl":"http://reviews.fingerhut.com/content/3526/termsandconditions.htm","cartName":"Cart","omnitureAccount":"fingerhutcomprod","bazaarVoiceApiConfig":{"baseUrl":"//api.bazaarvoice.com/data/","apiKey":"5ubws8wxe7211f2i2wzmnwtb2","userToken":null,"authorId":"0"},"id":null});

        // API
        /**
         * Will not override a property if it already exists.
         * @param property - property to set on pageProperties
         * @param value - value to set for provided property
         */
        pageProperties.safeAdd = function(property, value) {
            if (!pageProperties[property]) {
                pageProperties[property] = value;
            }
        };

        pageProperties.safeAddAll = function(properties) {
            for (var property in properties) {
                pageProperties.safeAdd(property, properties[property]);
            }

        };
  
    }

    // Return singleton
    return new PageProperties();
	
});
