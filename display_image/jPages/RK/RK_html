var bsb = bsb || {};
(function() {
    function TemplateSwapper() {
        var swapper = this;
        
        // API
        
        swapper.swap = function(toSwap) {
           if (!$.isArray(toSwap)) {
               toSwap = [toSwap];
           };
           
           for (var i in toSwap) {
               var each = toSwap[i];
               $('#' + each.oldId).remove();
               $('#' + each.newId).attr('id', each.oldId);
           }
        };
    }
    
    // Return singleton.
    bsb.templateSwapper = new TemplateSwapper();
})();
