                                            s.eVar55 = 'fb:like_' + familyId + '_' + topLine;
                                            s.events = 'event43';
                                            s.tl(this, 'o', 'Exit to FBLikeProduct');
                                        }
                                        break;    
                                        //pinterest clickthrough is not being called if the custom pinit control is used.                            
                                    case 'pinterest_pinit':
                                        s.linkTrackVars = 'prop45,eVar55,events';
                                        s.linkTrackEvents = 'event43';
                                        s.prop45 = 'pinterest:pin_' + familyId + '_' + topLine;
                                        s.eVar55 = 'pinterest:pin_' + familyId + '_' + topLine;
                                        s.events = 'event43';
                                        s.tl(this, 'o', 'Exit to PinterestPinProduct');
                                        return true;
                                        break;
