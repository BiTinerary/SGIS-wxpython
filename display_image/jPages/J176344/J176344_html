                                        s.linkTrackVars = 'prop45,eVar55,events';
                                        s.linkTrackEvents = 'event43';
                                        s.prop45 = 'twitter:share_' + familyId + '_' + topLine;
                                        s.eVar55 = 'twitter:share_' + familyId + '_' + topLine;
                                        s.events = 'event43';
                                        s.tl(this, 'o', 'Exit to TwitterShareProduct');
                                        break;
                                    default:
                                        //do nothing
                                }
                            }
                        }
                        // Listen for the share event
                        addthis.addEventListener('addthis.menu.share', shareEventHandler);
                        var addthis_config = addthis_config || {};
                        addthis_config.data_track_clickback = false;
