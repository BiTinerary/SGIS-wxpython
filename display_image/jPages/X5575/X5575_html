        // API
        /**
         * Will not override a property if it already exists.
         * @param property - property to set on pageProperties
         * @param value - value to set for provided property
         */
        pageProperties.safeAdd = function(property, value) {
            if (!pageProperties[property]) {
                pageProperties[property] = value;
            }
        };

        pageProperties.safeAddAll = function(properties) {
            for (var property in properties) {
                pageProperties.safeAdd(property, properties[property]);
            }

        };
  
    }

    // Return singleton
    return new PageProperties();
	
