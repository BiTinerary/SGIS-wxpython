<script type="text/javascript">
    $(document).ready(function () {
        /* Setup the why? link */
        $(".js-whyLink").click(function(evt) {
            evt.preventDefault();
            var url = $(this).attr("href").split(".jsp");
            url = url[0] + "_body.jsp" + url[1];
            site.func.openGenericLayer(url);
        });
        /* ---------- */
    });

</script>

<script type="text/javascript">
        $(document).ready(function () {
            $("#signInLaunch").click(function(e) {
                e.preventDefault();
                var contextPath = "";
                var url = window.location.href;
                var path = window.location.pathname;
                path = path.substring(contextPath.length, path.length);
                var destPath = '/user/login.jsp';
                var hasDest = false;

                if (path != "/user/login.cmd") {
                    var posQ = url.indexOf("?");
                    if (posQ > 0) {
                        if (url.indexOf("dest=") > 0) {
                            hasDest = true;
                            var protocol = window.location.protocol + "//";
                            path = url.substring(url.indexOf(path, protocol.length));
                        } else {
                            var reqQs = url.substring(posQ + 1);
                            var paramStr = "";
                            var reqParams = reqQs.split("&");
                            for (var i = 0; i < reqParams.length; i++) {
                                var token = reqParams[i];
                                if (paramStr.length > 0) {
                                    paramStr = paramStr + "|"; //replace | with & before actual forward
                                }
                                paramStr = paramStr + token;
                            }

                            if (paramStr.length > 0) {
                                path = path + "||" + paramStr; //replace || with ? before actual forward
                            }
                        }
                    }

                    if (hasDest) {
                        destPath = contextPath + path;
                    } else {
                    	if(path.indexOf("sdstgewww") > 0){
							if (path == "/sdstgewww/user/logout.cmd" || path == "/sdstgewww/user/logout.jsp" || path == "/sdstgewww/user/login.jsp") {
								path = "/sdstgewww/user/main.jsp";
							}
							destPath = "https://" + window.location.hostname + "/sdstgewww" + destPath + "?dest=" + path;
						}else if(path.indexOf("sdtestwww") > 0){
							if (path == "/sdtestwww/user/logout.cmd" || path == "/sdtestwww/user/logout.jsp" || path == "/sdtestwww/user/login.jsp") {
								path = "/sdtestwww/user/main.jsp";
							}
							destPath = "https://" + window.location.hostname + "/sdtestwww" + destPath + "?dest=" + path;
						}else{
							if (path == "/user/logout.cmd" || path == "/user/logout.jsp" || path == "/user/login.jsp") {
								path = "/user/main.jsp";
							}
							destPath = "https://" + window.location.hostname + destPath + "?dest=" + path;
						}
                    }
                }
                window.location.href = destPath;
            });
        });
    </script>
