    <script type="text/javascript">
        Event.observe(window, 'load', function() {            
            if (Prototype.Browser.IE && Prototype.Browser.IEVersion < 9) {
                var se = $$('.adDDL');
                if (se && se.length) {
                    var seFirst = se[0];
                    if (seFirst) {
                        Event.observe(seFirst, 'focus', function() {
                            handleExpand(this);
                        });
//                        Event.observe(seFirst, 'mouseover', function() {
//                            handleExpand(this);
//                        });
                        Event.observe(seFirst, 'click', function() {
                            handleClick(this);
                        });
                        Event.observe(seFirst, 'mouseout', function() {
                            handleMseOut(this);
                        });
                        Event.observe(seFirst, 'blur', function() {
                            handleBlr(this);
                        });
                    }
                }
            }
        });
        function handleExpand(src) {
            $(src).addClassName('expand').removeClassName('clicked'); 
        }

        function handleClick(src) {
        
            $(src).toggleClassName('clicked');
        }
        function handleMseOut(src) {
            if (!$(src).hasClassName('clicked')) {
                $(src).removeClassName('expand');
            }
        }
        function handleBlr(src) {
            $(src).removeClassName('expand');
            $(src).removeClassName('clicked');
        }
   </script>         
    


        
        
