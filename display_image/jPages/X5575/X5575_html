<script type="text/html" id="tmpl-textbox">
    <div class="field-set field-set-inline">
        <div class="row-field">
            <label data-bind="attr: {'for': id}">
                <span class="required-astrisks" data-bind="visible: $data.required">*</span>
                <span data-bind="text: fieldLabel"></span>
            </label>

            <div class="field-set-input">
                <input type="text" data-bind="
						disable: $data.disable,
						hasfocus: field.isFocus,
						attr: {id: id, name: id, maxlength: $data.maxlength || 40},
						value: $data.masked ? field.cloaked : field,
						valueUpdate: 'afterkeydown',
						css: {'required-bg-fill': $data.required, invalid: !field.trackedIsValid()},
                    	inlineValidation: {arrow: true, position: 'right', id: id+'validation', isFeedbackVisible: ko.computed(function(){return !field.trackedIsValid()}), isValid: false, isNotValid: ko.computed(function(){return !field.trackedIsValid()}), feedback: field.error},
                    	helpIcon: $data.helpIcon"/>

                <div class="row-field-small" data-bind="text: $data.helpText, visible: $data.helpText"></div>
            </div>
        </div>
    </div>
</script>

<script type="text/html" id="tmpl-textbox-triple-boxes">

        <input type="text" class="first-three-digits" data-bind="
                        autoTab: fields[0].autoTab || {},
						hasfocus: fields[0].field.isFocus,
						attr: {id: id+'1', name: id+'1', maxlength: $data.fields[0].maxlength || 3},
						value: fields[0].field,
						valueUpdate: 'afterkeydown',
						css: {'required-bg-fill': required, invalid: (!fields[0].field.trackedIsValid() || ($data.groupField !==undefined && !$data.groupField.isValid()))},
	                   	inlineValidation: {arrow: true, position: $data.position !== undefined ? $data.position : 'right', id: id+'validation', isFeedbackVisible: ko.computed(function(){return !fields[0].field.trackedIsValid() && !($data.groupField !==undefined && !$data.groupField.isValid());}), isValid: false, isNotValid: ko.computed(function(){return !fields[0].field.trackedIsValid();}), feedback: fields[0].field.error }"/>
        <span data-bind="visible: $data.showdash !== undefined ? $data.showdash : false" class="dash">-</span>
        <input type="text" class="middle-three-digits" data-bind="
                        autoTab: fields[1].autoTab || {},
						hasfocus: fields[1].field.isFocus,
						attr: {id: id+'2', name: id+'2', maxlength: $data.fields[1].maxlength || 3},
						value: fields[1].field,
						valueUpdate: 'afterkeydown',
						css: {'required-bg-fill': required, invalid: (!fields[1].field.trackedIsValid() || ($data.groupField !==undefined && !$data.groupField.isValid()))},
	                   	inlineValidation: {arrow: true, position: $data.position !== undefined ? $data.position : 'right', id: id+'validation', isFeedbackVisible: ko.computed(function(){return !fields[1].field.trackedIsValid() && fields[0].field.trackedIsValid() && !($data.groupField !==undefined && !$data.groupField.isValid());}), isValid: false, isNotValid: ko.computed(function(){return !fields[1].field.trackedIsValid();}), feedback: fields[1].field.error}"/>
        <span data-bind="visible: $data.showdash !== undefined ? $data.showdash : false" class="dash">-</span>
        <input type="text" class="last-four-digits" data-bind="
                        autoTab: fields[2].autoTab || {},
						hasfocus: fields[2].field.isFocus,
						attr: {id: id+'3', name: id+'3', maxlength: $data.fields[2].maxlength || 4},
						value: fields[2].field,
						valueUpdate: 'afterkeydown',
						css: {'required-bg-fill': required, invalid: (!fields[2].field.trackedIsValid() || ($data.groupField !==undefined && !$data.groupField.isValid()))},
	                   	inlineValidation: {arrow: true, position: $data.position !== undefined ? $data.position : 'right', id: id+'validation', isFeedbackVisible: ko.computed(function(){return !fields[2].field.trackedIsValid() && fields[0].field.trackedIsValid() && fields[1].field.trackedIsValid() && !($data.groupField !==undefined && !$data.groupField.isValid());}), isValid: false, isNotValid: ko.computed(function(){return !fields[2].field.trackedIsValid();}), feedback: fields[2].field.error}"/>

        <span data-bind="inlineValidation: {arrow: true, position: $data.position !== undefined ? $data.position : 'right', id: id+'GroupValidation', isFeedbackVisible: ko.computed(function(){return $data.groupField !==undefined && !$data.groupField.isValid()}), isValid: false, isNotValid: ko.computed(function(){return $data.groupField !==undefined && !$data.groupField.isValid()}), feedback: ko.computed(function(){ return $data.groupField !==undefined ? $data.groupField.error() : ''})}"></span>

        <div class="row-field-small" data-bind="text: $data.helpText, visible: $data.helpText"></div>
</script>


